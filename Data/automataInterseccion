digraph {
	rankdir=LR
	size="8,5"
	start [shape=point]
	AC [shape=circle]
	start -> AC
	AC [shape=circle]
	AD [shape=circle]
	BC [shape=circle]
	BD [shape=doublecircle]
	AC -> AD [label=1]
	AC -> BC [label=0]
	AD -> AD [label=1]
	AD -> BD [label=0]
	BC -> BD [label=1]
	BC -> BC [label=0]
	BD -> BD [label=1]
	BD -> BD [label=0]
}
