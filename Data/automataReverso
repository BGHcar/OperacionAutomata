digraph {
	rankdir=LR
	size="8,5"
	start [shape=point]
	BD [shape=circle]
	start -> BD
	BD [shape=circle]
	"AC;AD;BC;BD" [shape=doublecircle]
	"BC;BD" [shape=circle]
	"AD;BD" [shape=circle]
	BD -> "AD;BD" [label=0]
	BD -> "BC;BD" [label=1]
	"AC;AD;BC;BD" -> "AC;AD;BC;BD" [label=0]
	"AC;AD;BC;BD" -> "AC;AD;BC;BD" [label=1]
	"BC;BD" -> "AC;AD;BC;BD" [label=0]
	"BC;BD" -> "BC;BD" [label=1]
	"AD;BD" -> "AD;BD" [label=0]
	"AD;BD" -> "AC;AD;BC;BD" [label=1]
}
