digraph {
	rankdir=LR
	size="8,5"
	start [shape=point]
	AC [shape=doublecircle]
	AD [shape=circle]
	BC [shape=circle]
	BD [shape=circle]
	start -> BD
	BD [shape=circle]
	BC -> AC [label=0]
	AD -> AC [label=1]
	BD -> AD [label=0]
	AD -> AD [label=1]
	BC -> BC [label=0]
	BD -> BC [label=1]
	BD -> BD [label=0]
	BD -> BD [label=1]
}
